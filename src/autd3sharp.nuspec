<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>$id$</id>
    <version>$version$</version>
    <authors>$authors$</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>$description$</description>
    <dependencies>
      <group targetFramework=".NETStandard2.0">
        <dependency id="NETStandard.Library" version="2.0.3" exclude="Build,Analyzers" />
      </group>
    </dependencies>
    <repository type="git" url="https://github.com/shinolab/autd3sharp.git" branch="v0.3" />
  </metadata>
  <files>
    <file src="bin\x64\$config$\netstandard2.0\$id$.dll" target="lib\netstandard2.0" />
    <file src="native\windows\x64\*.dll" target="runtimes\win-x64\native" />
    <file src="native\linux\x64\*.so" target="runtimes\ubuntu-x64\native" />
    <file src="native\osx\x64\*.dylib" target="runtimes\osx-x64\native" />
  </files>
</package>